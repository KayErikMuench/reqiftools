		<artifactId>reqif-parent</artifactId>
		<version>0.0.1-SNAPSHOT</version>
		<relativePath>../reqiftools-parent</relativePath>
	</parent>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
